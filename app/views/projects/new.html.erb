<h1>New Project</h1>


<%= form_for @project, method: 'POST' do |xx| %>
  <label> user </label>
  <%= xx.text_field :user_id %>
  <label> Title </label>
  <%= xx.text_field :title %>
  <label> Info </label>
  <%= xx.text_field :info %>
  <label> Goal </label>
  <%= xx.text_field :goal %>
  <label> Days to go  </label>
  <%= xx.text_field :daystogo %>

  <%= xx.submit "Create Project" %>
<% end %>

<%= link_to 'Back', projects_path %>
