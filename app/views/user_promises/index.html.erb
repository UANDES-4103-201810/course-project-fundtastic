<p id="notice"><%= notice %></p>

<h1>User Promises</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_promises.each do |user_promise| %>
      <tr>
        <td><%= link_to 'Show', user_promise %></td>
        <td><%= link_to 'Edit', edit_user_promise_path(user_promise) %></td>
        <td><%= link_to 'Destroy', user_promise, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User Promise', new_user_promise_path %>
